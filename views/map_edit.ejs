<!DOCTYPE html>
<html>
<head lang="ja">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>TraVerse</title>
    <link rel="icon" href="../images/freedrink.ico">
    <!-- <link rel="stylesheet" href="css/style.css"> -->
    <link rel="stylesheet" href="css/create_map.css">
    <!-- <link rel="stylesheet" href="css/create.css"> -->
    <script type="text/javascript" src="//code.jquery.com/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="https://cdn.webrtc.ecl.ntt.com/skyway-latest.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

</head>


<body>
    <header>
        <section class="flex" id="header-menu">
            <div class="headerTitle center">TraVerse</div>
            <div class="openbtn">
                <span></span><span>Menu</span><span></span>
            </div>
        </section>
    </header>
            <!-- ハンバーガーメニュー -->
            <nav id="g-nav">
                <div class="menuHamburgerMenuTopSizedBox"></div>
                <div id="g-nav-list">

                </div>
            </nav>
    <div id="main-box">
        <canvas id="new-create"></canvas>
        <div id="sub-box">
            <div class="tabs">
                <input id="design-1" type="radio" name="tab_item" checked>
                <label class="tab_item" for="design-1">デザイン１</label>
                <input id="design-2" type="radio" name="tab_item">
                <label class="tab_item" for="design-2">デザイン２</label>
                <input id="design-3" type="radio" name="tab_item">
                <label class="tab_item" for="design-3">デザイン３</label>


                <div class="tab_content" id="design-1-Content">
                    <div class="tab_content_descripstion">
                        <div class="center">
                            <section id="content-1" class="loginMarginTitleTop">
                                <p class="my_japanese_font loginMarginBottom center">デザイン１</p>
                                <div class="space"></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-0"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-1"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-2"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-3"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-4"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-5"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-6"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-7"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-8"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-9"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-10"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-11"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-12"></canvas></div>
                            </section>
                        </div>
                    </div>
                </div>
                <div class="tab_content" id="design-2-Content">
                    <div class="tab_content_descripstion">
                        <div class="center">
                            <section id="content-2" class="loginMarginTitleTop loginSectionSize">
                                <p class="my_japanese_font loginMarginBottom center">デザイン２</p>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-13"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-14"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-15"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-16"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-17"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-18"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-19"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-20"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-21"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-22"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-23"></canvas></div>
                                <div class= "canvas-hidden"><canvas class="edit" id="edit-24"></canvas></div>
                            </section>
                        </div>
                    </div>
                </div>
                <div class="tab_content" id="design-3-Content">
                    <div class="tab_content_descripstion">
                        <section id="content-3" class="loginMarginTop center">
                            <p class="my_japanese_font center loginMarginBottom">デザイン３</p>
                            <div class= "canvas-hidden"><canvas class="edit" id="edit-25"></canvas></div>
                        </section>
                    </div>
                </div>
            </div>

            <div id="detail">
                <input type="button" id="call-distinction" value="通話エリア選択">
                <!-- <input type="button" id="spawn-distinction" value="スポーン位置選択" onClick="spawn_position();"> -->
                <form name="dist_table" id="dist-table" class="hide_window" onsubmit="return false;">
                    <img class="close" id="dist-close" src="images/ui/close.svg">
                    <div id="add-box">
                        <input type="text" id="add-name">
                        <input type="button" id="add-button" value="追加">
                    </div>
                    <input type="button" id="delete" value="削除">
                </form>
                <form name="add_table" id="add-table" class="hide_window" onsubmit="return false;">
                    <img class="close" id="add-close" src="images/ui/close.svg" >
                </form>
                <form name="move_map" id="move-map" class="hide_window" onsubmit="return false;">
                    <img class="close" id="move-close" src="images/ui/close.svg">
                </form>
                <!-- <button id="move-map">マップ移動選択</button> -->
            </div>


            <form id="create_form">
            </form>
        </div>
    </div>
    <script type="text/javascript" src="js/page_design.js"></script>
    <script type="text/javascript" src="js/<%= file_value %>.js"></script>


</body>
</html>


